{
  "name": "smart-garden",
  "version": "1.0.0",
  "description": "A smart garden server to manage some parameters",
  "main": "app.js",
  "scripts": {
    "test": "mocha --recursive --compilers js:babel-core/register",
    "webpack-watch": "webpack -w",
    "serve": "node ./lib/app",
    "dev": "concurrent --kill-others \"npm run webpack-watch\" \"npm run serve\"",
    "build": "webpack --config webpack.config.js --progress --profile --colors"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/carrascoacd/smart-garden.git"
  },
  "author": "Antonio Carrasco",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/carrascoacd/smart-garden/issues"
  },
  "homepage": "https://github.com/carrascoacd/smart-garden#readme",
  "dependencies": {
    "babel-core": "^6.18.2",
    "babel-loader": "^6.2.7",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "body-parser": "^1.15.2",
    "express": "^4.13.4",
    "material-ui": "^0.16.1",
    "mongoose": "^4.4.6",
    "pug": "^2.0.0-beta6",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-easy-chart": "^0.1.12",
    "react-tap-event-plugin": "^1.0.0",
    "webpack": "^1.13.3"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "concurrently": "^3.1.0",
    "dotenv": "^2.0.0",
    "mitm": "^1.2.0",
    "mocha": "^2.4.5",
    "supertest": "^1.2.0"
  }
}
